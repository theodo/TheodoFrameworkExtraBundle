{
    "name": "theodo/framework-extra-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides some useful classes in your Symfony2 project",
    "keywords": ["tests"],
    "homepage": "http://www.theodo.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "Theodo",
            "homepage": "http://www.theodo.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1.0,<3.0",
        "doctrine/orm": "~2.2,>=2.2.3",
        "hautelook/alice-bundle": "dev-master"
    },
    "minimum-stability": "dev",
    "target-dir": "Theodo/Bundle/FrameworkExtraBundle",
    "autoload": {
        "psr-0": {
            "Theodo\\Bundle\\FrameworkExtraBundle": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
